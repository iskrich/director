--
set x = 1
if x = 5 then exit
else put 10.0

repeat with x = 1 to 5
	if x = 3 then put 30
	else if x = 4 then put 40
	else if x = 5 then put 50
	else put 10.0
	if x = 1 then
	  put 1
	else if x = 2 then
	  put 1232.12345678901234
	  put 2.2
	else if x = 3 then put 3
	end if
	if x = 4 then put 4
	else put 5
end repeat
